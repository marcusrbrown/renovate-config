{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  description: 'Use the config preset for the @marcusrbrown/renovate-config repository',
  extends: ['github>marcusrbrown/renovate-config#v1.15.9'],
  packageRules: [
    {
      matchDepNames: ['bfra-me/renovate-config'],
      matchPackageNames: [
        '@vercel/ncc',
        'conventional-changelog-conventionalcommits',
        'semantic-release',
        'typescript',
      ],
      matchPackagePrefixes: ['@semantic-release/'],
      semanticCommitType: 'build',
    },
  ],
  postUpgradeTasks: {
    commands: [
      'corepack enable',
      'pnpm install --frozen-lockfile --ignore-scripts',
      'git checkout -- .npmrc',
      'pnpm run format',
    ],
    executionMode: 'branch',
  },
  customManagers: [
    {
      customType: 'regex',
      datasourceTemplate: 'github-releases',
      fileMatch: ['^archived-repository\\.json$', '^default\\.json$', '^onboarding\\.json$', '^personal\\.json$'],
      matchStrings: [
        '["\']github>(?<depName>(bfra-me|marcusrbrown)\\/renovate-config)(?:(?::|\\/\\/)(?<path>.+?))?#(?<currentValue>.+?)["\']',
      ],
      versioningTemplate: 'semver',
    },
  ],
}
