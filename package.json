{
  "name": "@marcusrbrown/renovate-config",
  "version": "0.0.0-development",
  "private": true,
  "description": "Shareable Renovate configuration presets for my personal GitHub repositories.",
  "keywords": [
    "renovate-config"
  ],
  "homepage": "https://github.com/marcusrbrown/renovate-config#readme",
  "bugs": {
    "url": "https://github.com/marcusrbrown/renovate-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcusrbrown/renovate-config.git"
  },
  "license": "MIT",
  "author": "Marcus R. Brown <contact@marcusrbrown.com> (https://marcusrbrown.com/)",
  "type": "module",
  "main": "default.json",
  "scripts": {
    "bootstrap": "pnpm install --prefer-offline --loglevel warn",
    "lint": "eslint",
    "fix": "eslint --fix",
    "prepare": "simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*.{js,json,jsx,md,toml,ts,tsx,yml,yaml}": [
      "eslint --fix"
    ]
  },
  "prettier": "@bfra.me/prettier-config/120-proof",
  "devDependencies": {
    "@bfra.me/eslint-config": "0.49.0",
    "@bfra.me/prettier-config": "0.16.7",
    "conventional-changelog-conventionalcommits": "9.1.0",
    "eslint": "10.0.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "lint-staged": "16.2.7",
    "markdownlint": "0.39.0",
    "prettier": "3.8.1",
    "semantic-release": "25.0.3",
    "semantic-release-export-data": "1.2.0",
    "simple-git-hooks": "2.13.1"
  },
  "packageManager": "pnpm@10.30.1"
}
